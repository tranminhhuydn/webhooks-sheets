<h1>Projects</h1>
<div class="container text-center">
    <div class="row card-header">
        <div class="col-2">id</div>
        <div class="col-6">name</div>
        <div class="col-2">status</div>
        <div class="col-2"><a class="btn btn-primary" href="/<%=controller%>/add">Create Project</a></div>
    </div>
    <% data.map((e,i)=>{
        if(i!=0 && e[0]!=='id'){
        %>
        <div class="row">
            <div class="col-2"><%=e[0]%></div>
            <div class="col-6"><%=e[1]%></div>
            <div class="col-2"><%=e[3]%></div>
            <div class="col-2"><a href="#" class="btn btn-primary">Join</a></div>
        </div>
        <%}})%>
</div>